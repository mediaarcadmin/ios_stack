iginY="320.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="down." Fill="#FF000000" /><Canvas RenderTransform="1,0,0,1,64,330"><Glyphs OriginX="16" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="Hoping to find a female ally, I called out to my mother" Indices=";;;;;;,31;;;,31;(2)633;;;,31;;,31;;;;;;;,31;;;;;;,31;;,31;;;;;;;,31;;;;,31;;;,31;;;,31" Fill="#FF000000" /><Glyphs OriginX="407.846666666667" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="412.963333333333" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="in" Fill="#FF000000" /><Glyphs OriginX="426.883333333333" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="-" Fill="#FF000000" /></Canvas><Glyphs OriginX="64" OriginY="362.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="law." Fill="#FF000000" /><Canvas RenderTransform="1,0,0,1,64,372"><Glyphs OriginX="16" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="“" Fill="#FF000000" /><Glyphs OriginX="23.25" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/00e848da-9e47-40b3-a1d3-8a3326691d05.ODTTF" UnicodeString="Amma!" Fill="#FF000000" /><Glyphs OriginX="77.2733333333333" OriginY="13.8966666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="Auntie!”" Fill="#FF000000" /></Canvas><Glyphs OriginX="80" OriginY="408.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="There was no reply. I screamed again." Fill="#FF000000" /><Glyphs OriginX="80" OriginY="431.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="“Somebody help me!”" Fill="#FF000000" /><Glyphs OriginX="80" OriginY="454.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="When he took off his white tunic, I rolled into a ball to " Indices=";;;;,43;;;,43;;;;;,43;;(2)643;,43;;;;,43;;;;;;,43;;;;;;;,43;;,43;;;;;;;,43;;;;;,43;;,43;;;;;,43" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="473.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="protect myself, but he began pulling at my nightshirt, " Indices=";;;;;;;,70;;;;;;;;,70;;;;,70;;;,70;;;;;;,70;;;;;;;;,70;;;,70;;;,70" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="492.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="wanting me to undress. Then he ran his rough hands over " Indices=";;;;;;;,43;;;,43;;;,43;;;;;;;;;,43;;;;;,43;;;,43;;;;,43;;;;,43;;;;;;,43;;;;;;,43" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="511.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="my body and pressed his lips against mine. He smelled so " Indices=";;,45;;;;;,45;;;;,45;;;;;;;;,45;;;;,45;;;;;,45;;;;;;;;,45;;;;;;,45;;;,45;;;;;;;;,45" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="530.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="awful, a mixture of tobacco and onion." Fill="#FF000000" /><Glyphs OriginX="80" OriginY="553.896666666667" FontRenderingEmSize="16" FontUri="/Resources/71423bab-b95d-4eaf-a3e1-02e8fa6a8e47.ODTTF" UnicodeString="I tried to get away again, moaning, “Get away from me! " Indices=";,41;;;;;;,
;;;;;,35;;;;;,35;;;;;,35;;;;;,35;;;;,35;;;;;;,35;;;;;;;,35;;;;;;;;;;,35;;;;;;,35;;;;;,35" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="280.566666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="varying degrees of indifference about the prospect of working with you." Indices=";;;;;;;;;;;;;;,37;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,37" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="296.316666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Worse yet, the people you dislike are not necessarily the same ones who dislike you. " Indices=";;;,37;;,31;;;;;,31;;;;,31;;;;;;;,31;;;;,31;;;,37;;;;;,31;;;;,31;;;;,31;;;;;,37;,37;;;;;;,31;;;;,31;,37;;;;,31;;;;,37;,31;;;;,31;;;,37;;;;;,31" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="312.066666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Just because you like someone does not mean they return the feeling. Even if the " Indices=";;,37;;,46;;;;;;,37;;,46;;;;,46;;;;;,46;,37;;;;;;;,46;;;;,37;,46;;;;,46;;;;;,46;;;;;,46;;;;;;;,46;;;;,46;;;;;;;;;,46;;;;;,46;;;,46" Fill="#FF000000" /><Canvas RenderTransform="1,0,0,1,48,315.75"><Glyphs OriginX="0" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="percentage of “non" Indices=";;;;;;;;;;,52;;;,52" Fill="#FF000000" /><Glyphs OriginX="112.27" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="116.643333333333" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="likes” is the same between professionals and clients, the total " Indices=";;;;,37;;,52;;,37;,52;;;;,52;,37;;;;,52;;;;;;;;,52;;;;;;,37;,37;;;;;;,37;,52;;;;,52;;;;;;;,37;;,52;;;;,52" Fill="#FF000000" /></Canvas><Canvas RenderTransform="1,0,0,1,48,331.5"><Glyphs OriginX="0" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="number of fits will be less. No wonder we have cases of non" Indices=";;;;;;,47;;;,47;;;;,37;,47;;;;;,47;;;,47;;;,37;,37;;,47;;;,47;;;;;;;,47;;;,47;;;;;,47;;;,37;;,37;,47;;;,47" Fill="#FF000000" /><Glyphs OriginX="364.966666666667" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="369.34" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="fit! Some nontrivial " Indices=";;;;,47;;;;;,47" Fill="#FF000000" /></Canvas><Glyphs OriginX="48" OriginY="359.316666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="percentage of potential relationships just will not work." Indices=";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,37;;;;,37;;;;,37" Fill="#FF000000" /><Canvas RenderTransform="1,0,0,1,48,363"><Glyphs OriginX="16" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="A good rule to remember is that, in relationships, there are no win" Indices=";,33;;;;;,33;;;;;,33;;;,33;;;;;;;;;,33;;,37;,33;;;;;;,33;;;,33;;;;;;;;;,37;;;;,37;;,33;;;;;;,33;;;;,33;;;,33" Fill="#FF000000" /><Glyphs OriginX="387.953333333333" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="392.326666666667" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="lose or lose" Indices=";;,37;;,33;;;,33;;;,37" Fill="#FF000000" /><Glyphs OriginX="455.946666666667" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="460.32" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="win " Fill="#FF000000" /></Canvas><Canvas RenderTransform="1,0,0,1,48,378.75"><Glyphs OriginX="0" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="combinations: There are only win" Indices=";;;;;;;;;;;,37;;,40;,62;;;;;,40;;;;,40;;;;;,40" Fill="#FF000000" /><Glyphs OriginX="190.506666666667" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="194.88" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="wins and lose" Indices=";;;,37;,40;;;;,40;;;,37" Fill="#FF000000" /><Glyphs OriginX="272.056666666667" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="-" Fill="#FF000000" /><Glyphs OriginX="276.43" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="loses. If the fit is not there for one " Indices=";;,37;;,37;;,40;;;,40;;;;,40;;;;,40;;,37;,40;;;;,40;;;;;;,40;;;;,40" Fill="#FF000000" /></Canvas><Glyphs OriginX="48" OriginY="406.566666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="party, then, just as for couples who ultimately divorce, it will, in the end, not work for " Indices=";;;;;;,33;;;;;;,33;;;,37;;,33;;,37;,33;;;;,33;;;;;;;,37;,33;;;;,33;;;;;;;;;;;,33;;;;;;;;;,33;;;,33;;;;;;,33;;;,33;;;;,33;;;;;,33;;;;,33;;;;;,33" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="422.316666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="either of them. To speak the truth about the disparity may be difficult, but it is usually " Indices=";;;;;;,32;;;,32;;;;;;,32;,62;;,32;,37;;;;;,32;;;;,32;;;;;;,32;;;;;;,32;;;;,32;;;,37;;;;;;;,32;;;;,32;;;,32;;;;;;;;;;;,32;;;;,32;;;,32;;,37;,32;;,37" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="438.066666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="the most efficient way out, not to mention the kindest." Indices=";;;;;;,37;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,37" Fill="#FF000000" /><Glyphs OriginX="241.833333333333" OriginY="484.55" FontRenderingEmSize="12" FontUri="/Resources/639dc035-744d-465f-b012-5fd2ab798426.ODTTF" UnicodeString="Client or Friend?" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="516.216666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Does all of this mean you have to make every client your friend? Not at all. You can be " Indices=";;;,37;,28;;;;,28;;;,28;;;;,37;,28;;;;;,28;;;;,28;;;;;,28;;;,28;;;;;,28;;;;;;,28;;;;;;;,28;;;;;,28;;;;;;;,37;,28;;;;,28;;;,28;;;;;,28;;;;,28;;;;,28" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="531.966666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="interested in someone without being their friend. You can deal with them as individual " Indices=";;;;;;,37;;;;,31;;;,31;,37;;;;;;;,31;;;;;;;;,31;;;;;;,31;;;;;;,31;;;;;;;;,31;;;;,31;;;;,31;;;;;,31;;;;;,31;;;;;,31;;,37;,31" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="547.716666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="human beings, and avoid treating them as people in a role, without pretending that they " Indices=";;;;;,28;;;;;;,37;;,28;;;;,28;;;;;;,28;;;;;;;;;,28;;;;;,28;;,37;,28;;;;;;;,28;;;,28;;,28;;;;;;,28;;;;;;;;,28;;;;;;;;;;;,28;;;;;,28" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="563.466666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="are your bosom buddy. Clients quickly see through these false friendships, often built on " Indices=";;;;;;;;;;;,37;;;;;;;;;;;;;;;;;,37;;;;;;;;;;,37;;;;;;;;;;;;;;;,37;;;;;;,37;;;;;;;;;,37;;;;,37" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="579.216666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="extended conversations about golf, football, and similar topics." Indices=";;;;;;;;;;;;;;;,37;;;;;;,37;;;;;;;;;;;;;;;;;;;;;;;;;;;;,37;;;;;;;;;;;;;,37" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="594.966666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Many professionals worry that it may be “unprofessional” to get that close to a client. " Indices=";;;;,26;;;;;;,37;,37;;;;;;,37;,26;;;;;;,26;;;;;,26;;;,26;;;;,26;;;,26;;;;;;;;;,37;,37;;;;;;;,26;;;,26;;;;,26;;;;;,26;;;;,37;;,26;;;,26;;,26" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="610.716666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="We don’t agree. Showing interest in the person does not mean intruding into private " Indices=";;,38;;;;;;,38;;;;;;;,38;;;;;;;;,38;;;;;;;,37;;,38;;;,38;;;;,38;;;;,37;;;,38;;;;,37;,38;;;;,38;;;;;,38;;;;;;;;;;,38;;;;;,38" Fill="#FF000000" /><Canvas RenderTransform="1,0,0,1,48,614.4"><Glyphs OriginX="0" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="areas. We think it is unprofessional " Indices=";;;;,37;;,33;;;,33;;;;;;,33;;;,33;;,37;,33;;;;;;;;,37;,37;;;;;;,33" Fill="#FF000000" /><Glyphs OriginX="199.63" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/626fc4e2-fd1e-4947-ab42-9e58a99d30d3.ODTTF" UnicodeString="not" Fill="#FF000000" /><Glyphs OriginX="218.686666666667" OriginY="12.0666666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="to show an interest in your client. To convince " Indices=";;,33;,37;;;;,33;;;,33;;;;;;;,37;;,33;;;,33;;;;;,33;;;;;;;;,33;,62;;,33" Fill="#FF000000" /></Canvas><Glyphs OriginX="48" OriginY="642.216666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="someone that they should view you as their trusted advisor, you must first convince " Indices=",37;;;;;;;,40;;;;;,40;;;;;,40;,37;;;;;;,40;;;;;,40;;;;,40;;,37;,40;;;;;;,40;;;;,37;;;;,40;;;;;,37;;;;,40;;;;,40;;;,37;;,40;;;;,37;;,40" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="657.966666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="them that you are committed to them. McDonald’s likes to describe professionals that " Indices=";;;;,33;;;;;,33;;;;,33;;;;,33;;;;;;;;;;,33;;;,33;;;;;;,33;;;;;;;;;;,37;,33;;;;;,37;,33;;;,33;;;,37;;;;;;,33;;;;;;,37;,37;;;;;;,37;,33" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="673.716666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="serve them (such as advertising agencies) as having “ketchup in their veins.”" Indices=",37;;;;;;;;;;;;,37;;;;;;,37;;;;;;;;;,37;;;;;;;;;;;;,37;;;;,37;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,37" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="689.466666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Does this mean that you actually have to care? Yes, you do actually have to care, if " Indices=";;;,37;,33;;;;,37;,33;;;;;,33;;;;;,33;;;;,33;;;;;;;;;,33;;;;;,33;;;,33;;;;;,37;,33;;;,37;;,33;;;;,33;;;,33;;;;;;;;;,33;;;;;,33;;;,33;;;;;;,33" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="705.216666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="you want to be a trusted advisor. If you want to be merely a vendor, you don’t." Indices=";;;;;;;;;;;;;;;;;;;;,37;;;;;;;;;,37" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="720.966666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Rob developed a great relationship with Arnold, a director of a major firm. This " Indices=",63;;;,42;;;;;;;;;;,42;;,42;;;;;;,42;;;;;;;;;,37;;;;,42;;;;;,42;;;;;;;;,42;;,42;;;;;;;;;,42;;;,42;;,42;;;;;;,42;;;;;;,42;,62;;;,37" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="736.716666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="hadn’t been easy, as Arnold was pretty demanding, intellectually and otherwise. When " Indices=";;;;;;,36;;;;;,36;;;,37;;;,36;;,37;,36;;;;;;;,36;;;,37;,36;;;;;;;,36;;;;;;;;;;;,36;;;;;;;;;;;;;;;,36;;;;,36;;;;;;;;,37;;;,36" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="752.466666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Arnold left the company, Rob found that it was much more difficult to be as committed " Indices=";;;;;;,26;;;;;,26;;;;,26;;;;;;;;;,26;,63;;;,26;;;;;;,26;;;;;,26;;;,26;;;,37;,26;;;;;,26;;;;;,26;;;;;;;;;;,26;;;,26;;;,26;;,37;,26" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="768.216666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="to the company relationship as he once was." Indices=";;;;;;;;;;;;;;;;;;;;;;;,37;;;;;;,37;;;;;;;;;;;;,37" Fill="#FF000000" /><Glyphs OriginX="64" OriginY="783.966666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="Rob tried really hard, certainly liked the people who replaced Arnold, and continued " Indices=",63;;;,30;;;;;;,30;;;;;;;,30;;;;;;,30;;;;;;;;;;,30;;;;;;,30;;;;,30;;;;;;;,30;;;;,30;;;;;;;;;,30;;;;;;;;,30;;;;,30" Fill="#FF000000" /><Glyphs OriginX="48" OriginY="799.716666666667" FontRenderingEmSize="14" FontUri="/Resources/cde380fc-a4ec-48bf-a562-178f3244840c.ODTTF" UnicodeString="to do good work with and for them. He was given more work to do, but his heart really " Indices=";;,27;;;,27;;;;;,27;;;;;,27;;;;;,27;;;;,27;;;;,27;;;;;;,27;;;,27;;;,37;,27;;;;;;,27;;;;;,27;;;;;,27;;;,27;;;;,27;;;;,27;;;,37;,27;;;;;;,27" Fill="#FF000000" /></FixedPage>